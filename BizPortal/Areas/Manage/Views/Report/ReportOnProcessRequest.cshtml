<!-- Function convert ot Eng formate. -->
<script type="text/javascript">

    function ConverToEngDate(dateobj)
    {
        if (dateobj == '' || dateobj == null)
        {
            return null;
        }
        var strDate = dateobj.split("/");
        var dd = strDate[0];
        var mm = strDate[1];
        var yy = parseInt(strDate[2]) - 543;
        var engFormate = yy + "-" + mm + "-" + dd;

        return engFormate;
    }

</script>

