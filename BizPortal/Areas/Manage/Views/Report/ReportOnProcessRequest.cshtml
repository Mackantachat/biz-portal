<!-- Custom CSS -->
<style>
    .panel-heading-custom {
        background-color: gray !important;
    }

    .chart-container {
        position: relative;
        margin: auto;
        height: 80vh;
        width: 80vw;
    }
</style>

<!-- Page Load -->
<script type="text/javascript">
    $(document).ready(function () {
        $('[name=yearShow]').html('ทั้งหมด');
        LoadPage();
        GetDataTable();
    });

    var AppOverViewChart;
    var AppSepByOrgChart;
    var AppSepByPermitChart;

    function GetDataTable()
    {
        $.ajax
        ({
            type: "GET",
            url: '@Url.Content("~/Api/v2/ReportOnProcessRequestController/GetApplicationRequest")',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
          success: function (data)
          {
              $("[name=onProcessRtpNbr]").html(Object.keys(data).length);
              $.each(data, function (i, item) {
                var rows = "<tr>" +
                    "<td>" + item.CreatedDate + "</td>" +
                    "<td>" + item.ApplicationRequestNumber + "</td>" +
                    "<td>" + item.PermitName + "</td>" +
                    "<td>" + item.IdentityName + "</td>" +
                    "<td>" + item.ExpectedFinishDate + "</td>" +
                    "<td>" + item.Status + "</td>" +
                    "<td>" + item.StatusOperation + "</td>" +
                    "</tr>";
                  $('#tbApplication').append(rows);
              }); //End of foreach Loop
              GetBarChart();
              GetAppSepOrg();
              GetAppSepApp();
              GetAppStatusCheck();
          }, //End of AJAX Success function

            failure: function (data) {
                alert(data.responseText);
            }, //End of AJAX failure function
            error: function (data) {
                alert(data.responseText);
            } //End of AJAX error function

        });
    }

    function GetBarChart()
    {
        $.ajax
        ({
            type: "GET",
            url: '@Url.Content("~/Api/v2/ReportOnProcessRequestController/GetAppStatusNbr")',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
          success: function (data)
          {
              SetChart(data);
          }, //End of AJAX Success function

            failure: function (data) {
                alert(data.responseText);
            }, //End of AJAX failure function
            error: function (data) {
                alert(data.responseText);
            } //End of AJAX error function

        });
    }

    function SetChart(json)
    {
        var ctx = document.getElementById('canvas').getContext('2d');
        var data = json;

        var region = [];
        var rev_value = [];

        try {
            data.map((item) => {
                rev_value.push(item.StatusNbr);
                region.push(item.StrStatus);
            });

                if (AppOverViewChart)
                {
                    AppOverViewChart.destroy();
                }

                AppOverViewChart = new Chart(ctx, {
                type: 'bar',
                data: {
                    labels: region,
                    datasets: [{
                        label: 'ภาพรวมคำขอ',
                        data: rev_value,
                        backgroundColor: [
                            'rgba(255, 99, 132, 0.2)',
                            'rgba(54, 162, 235, 0.2)',
                            'rgba(255, 206, 86, 0.2)',
                            'rgba(75, 192, 192, 0.2)',
                            'rgba(153, 102, 255, 0.2)',
                        ],
                        borderColor: [
                            'rgba(255, 99, 132, 1)',
                            'rgba(54, 162, 235, 1)',
                            'rgba(255, 206, 86, 1)',
                            'rgba(75, 192, 192, 1)',
                            'rgba(153, 102, 255, 1)',
                        ],
                        borderWidth: 1
                    }]
                },
                options: {
                    scales: {
                        yAxes: [{
                            ticks: {
                                beginAtZero: true
                            }
                        }]
                    }
                }
            });

        } catch (error) {
            console.log(error);
        }
    }

    function GetAppSepOrg()
    {
        $.ajax
        ({
            type: "GET",
            url: '@Url.Content("~/Api/v2/ReportOnProcessRequestController/GetAppSepByOrg")',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
          success: function (data)
          {
              SetChartAppSepOrg(data);
          }, //End of AJAX Success function

            failure: function (data) {
                alert(data.responseText);
            }, //End of AJAX failure function
            error: function (data) {
                alert(data.responseText);
            } //End of AJAX error function

        });
    }

    function DynamicColors()
    {
        var r = Math.floor(Math.random() * 255);
        var g = Math.floor(Math.random() * 255);
        var b = Math.floor(Math.random() * 255);
        return "rgb(" + r + "," + g + "," + b + ")";
    };

    function SetChartAppSepOrg(json)
    {
        debugger;
        var ctx = document.getElementById('sepByOrg').getContext('2d');
        var data = json;    
        var region = [];
        var rev_value = [];
        var bcolor = [];

        try {

            if (AppSepByOrgChart)
            {
                AppSepByOrgChart.destroy();
            }

            data.map((item) => {
                rev_value.push(item.AppSepOrg);
                region.push(item.OrgName);
                bcolor.push(DynamicColors());
            });
            
                AppSepByOrgChart = new Chart(ctx, {
                type: 'horizontalBar',
                data: {
                    labels: region,
                    datasets: [{
                        label: 'จำนวนคำขอ แบ่งตามหน่วยงาน',
                        data: rev_value,
                        backgroundColor: bcolor,
                        borderColor: bcolor,
                        borderWidth: 1
                    }]
                },
                options: {
                    scales: {
                        yAxes: [{
                            ticks: {
                                beginAtZero: true
                            }
                        }]
                    }
                }
            });

        } catch (error) {
            console.log(error);
        }
    }

    function GetAppSepApp()
    {
	    debugger;
	    $.ajax
	    ({
		    type: "GET",
		    url: '@Url.Content("~/Api/v2/ReportOnProcessRequestController/GetAppSepByApp")',
		    contentType: "application/json; charset=utf-8",
		    dataType: "json",
	      success: function (data)
          {
              SetChartAppSepApp(data);
	      }, //End of AJAX Success function

		    failure: function (data) {
			    alert(data.responseText);
		    }, //End of AJAX failure function
		    error: function (data) {
			    alert(data.responseText);
		    } //End of AJAX error function

	    });
    }

    function SetChartAppSepApp(json)
    {
        var ctx = document.getElementById('sepByPermit').getContext('2d');
        var data = json;

        var region = [];
        var rev_value = [];
        var bcolor = [];

        try {

            data.map((item) => {
                rev_value.push(item.AppNbr);
                region.push(item.AppName);
                bcolor.push(DynamicColors());
            });

            if (AppSepByPermitChart)
            {
                AppSepByPermitChart.destroy();
            }

                AppSepByPermitChart = new Chart(ctx, {
                type: 'horizontalBar',
                data: {
                    labels: region,
                    datasets: [{
                        label: 'จำนวนคำขอ แบ่งตามใบอนุญาต',
                        data: rev_value,
                        backgroundColor: bcolor,
                        borderColor: bcolor,
                        borderWidth: 1
                    }]
                },
                options: {
                    scales: {
                        yAxes: [{
                            ticks: {
                                beginAtZero: true
                            }
                        }]
                    }
                }
            });

        } catch (error) {
            console.log(error);
        }
    }

    function GetAppStatusCheck()
    {
	    $.ajax
        ({
            type: "GET",
            url: '@Url.Content("~/Api/v2/ReportOnProcessRequestController/GetAppStatusCheck")',
            contentType: "application/json; charset=utf-8",
            dataType: "json",
          success: function (data)
          {
              debugger;
            var rows = "<tr>" +
                "<td style=color:#6ACAFD; align=center><i class=glyphicon glyphicon-file></i>จำนวนคำขอ</td>"+
                    "<td  align=center>" + data.CHECK + "</td>" +
                    "<td align=center>" + data.PENDING + "</td>" +
                    "<td align=center>" + data.APPROVED_WAITING_PAY_FEE + "</td>" +
                    "<td align=center>" + data.PAID_FEE_CREATING_LICENSE + "</td>" +
                    "</tr>";
            $('#tbCheckStatus').append(rows);

          }, //End of AJAX Success function

            failure: function (data) {
                alert(data.responseText);
            }, //End of AJAX failure function
            error: function (data) {
                alert(data.responseText);
            } //End of AJAX error function

        });
        showReportLoading(false);
    }

    function LoadPage()
    {
        showReportLoading(true, "กำลังโหลด...");
        $.ajax({
            url: '@Url.Content("~/Api/V2/ReportOnProcessRequestController/GetReportObj")',
            type: "GET",
            cache: false,
            contentType: 'application/json; charset=utf-8',
            dataType: 'json',
            success: function (result) {
                insertParamDataListToSelect2(result);
            },
            error: function (xhr, status, error) {
                let err = eval("(" + xhr.responseText + ")");
                console.log(err.Message);
            }
        });

        $('.province').select2({
            allowClear: true,
            placeholder: 'ทั้งหมด',
            ajax: {
                url: '@Url.Content("~/Api/v2/Geo/Provinces")',
                dataType: 'json',
                delay: 250,
                data: function (params) {
                    return {
                        isMetro: '',
                        query: params.term
                    };
                },
                processResults: function (data, params) {
                    return data;
                },
                cache: true
            },
            width: '100%'
        })
        .on('change', function (e, params) {
            if (params != "init") {
                $(this).valid();
            }
            $('.district').val(null).trigger('change');
        });

        $('.district').select2({
            allowClear: true,
            placeholder: 'ทั้งหมด',
            ajax: {
                url: '@Url.Content("~/Api/v2/Geo/Amphurs")',
                dataType: 'json',
                delay: 250,
                data: function (params) {
                    return {
                        query: params.term,
                        pCode: $('.province').val()
                    }
                },
                processResults: function (data, params) {
                    return data;
                },
                cache: true
            },
            width: '100%'
        });

        function insertParamDataListToSelect2(data) {
            $(".query-ddl").each(function () {
                $(this).select2({
                    data: data[this.name] || [],
                    allowClear: true,
                    width: '100%',
                    placeholder: "ทั้งหมด",
                    //minimumResultsForSearch: -1
                });
            });
        }

    }

    $(function () {
        $('#timeStart').datepicker({
            autoclose: true,
            language: "th-th",
            format: 'dd/mm/yyyy',
            startView: 'decade',
            forceParse: false,
        });
    });

    $(function () {
        $('#timeEnd').datepicker({
            autoclose: true,
            language: "th-th",
            format: 'dd/mm/yyyy',
            startView: 'decade',
            forceParse: false,
        });
    });

    $('#timeStart').change(function () {
        var n = $('#timeEnd').val();
        if (n == null || n == '') {
            $('#timeEnd').val($('#timeStart').val());
        }
        else {
            var strDate = n.split("/");
            var dd = strDate[0];
            var mm = strDate[1];
