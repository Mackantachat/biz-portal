@using BizPortal.Utils.Helpers
<script id="APP_ACCOUNTING_SERVICE_MANAGER_SECTIONTemplate" type="text/x-handlebars-template">
    {{#xif " data.length > 0 " }}
    {{#each data}}
    <div class="row form-group pt10 pb10" style="margin: 0 0 5px 0 !important; border:solid 1px #000000; border-radius: 4px;">
        <div class="col-md-4">
            <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_ACCOUNTING_SERVICE.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_TITLE :</label>
            {{this.DROPDOWN_APP_ACCOUNTING_SERVICE_MANAGER_SECTION_TITLE_TEXT}}
        </div>
        <div class="col-md-4">
            <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_ACCOUNTING_SERVICE.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_FIRSTNAME :</label>
            {{this.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_FIRSTNAME}}
        </div>
        <div class="col-md-4">
            <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_ACCOUNTING_SERVICE.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_LASTNAME :</label>
            {{this.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_LASTNAME}}
        </div>
        <div class="col-md-6">
            <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_ACCOUNTING_SERVICE.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_ID :</label>
            {{this.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_ID}}
        </div>
        <div class="col-md-6">
            <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_ACCOUNTING_SERVICE.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_AUDITOR_ID :</label>
            {{this.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_AUDITOR_ID}}
        </div>
        <div class="col-md-6">
            <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_ACCOUNTING_SERVICE.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_TYPE :</label>
            {{this.DROPDOWN_APP_ACCOUNTING_SERVICE_MANAGER_SECTION_TYPE_TEXT}}
        </div>
        <div class="col-md-6">
            <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_ACCOUNTING_SERVICE.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_VAT_DATE :</label>
            {{this.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_VAT_DATE}}
        </div>
        <div class="col-md-12">
            {{#xif " typeof this.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_WORKING_TYPE_OPTION !== 'undefined' " }}
            {{#xif " this.APP_ACCOUNTING_SERVICE_MANAGER_SECTION_WORKING_TYPE_OPTION == 'FULLTIME' " }}
            <label>รูปแบบปฏิบัติงาน :</label> เต็มเวลา <br />
            {{else}}
            <label>รูปแบบปฏิบัติงาน :</label> บางเวลา <br />
            {{/xif}}
            {{/xif}}
        </div>
        <br />
    </div>

    {{/each}}
    {{else}}
    <div class="row form-group pt10 pb10" style="margin: 0 0 5px 0 !important; border:solid 1px #000000; border-radius: 4px;">
        <div class="col-md-12 text-center">
            @Html.Raw(ResourceHelper.GetResourceWordWithDefault(Model.EmptyDataMessage, StringHelper.DefaultIfEmpty(Model.ResourceName, "Apps_SingleForm_Template"), Model.EmptyDataMessage))
        </div>
    </div>
    {{/xif}}
</script>