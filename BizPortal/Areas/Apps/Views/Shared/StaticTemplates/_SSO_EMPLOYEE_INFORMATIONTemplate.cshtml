@using BizPortal.Utils.Helpers
<script id="SSO_EMPLOYEE_INFORMATIONTemplate" type="text/x-handlebars-template">
    {{#xif " data.length > 0 " }}
    {{#each data}}

    <div class="row form-group pt10 pb10" style="margin: 0 0 5px 0 !important; border:solid 1px #000000; border-radius: 4px;">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-6">
                    <p class="form-control-static">
                        <label>เลขบัตรประจำตัวประชาชน 13 หลัก: </label> {{this.SSO_EMPLOYEE_CITIZEN_ID}}
                    </p>
                </div>
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " fieldExist(this.SSO_EMPLOYEE_REG_STATUS) && this.SSO_EMPLOYEE_REG_STATUS != 'null' " }}
                        <label>ชื่อ: </label>
                        {{#xif "this.SSO_EMPLOYEE_REG_STATUS == 'true'" }}
                        {{this.SSO_EMPLOYEE_TITLE_TRUE_TEXT}} {{this.SSO_EMPLOYEE_NAME_TRUE}} {{this.SSO_EMPLOYEE_LASTNAME_TRUE}}
                        {{else}}
                        {{this.SSO_EMPLOYEE_TITLE_FALSE_TEXT}} {{this.SSO_EMPLOYEE_NAME_FALSE}} {{this.SSO_EMPLOYEE_LASTNAME_FALSE}}
                        {{/xif}}
                        {{/xif}}
                    </p>
                </div>
            </div>
        </div>
        <div class="col-md-12 collapse" id="ssoCollapse{{@@index}}">
            {{#xif " fieldExist(this.SSO_EMPLOYEE_REG_STATUS) && this.SSO_EMPLOYEE_REG_STATUS != 'null' " }}
