                        <label>หมู่ใหญ่ : </label> {{this.AJAX_DROPDOWN_SSO_EMPLOYEE_TSIC_3_TEXT}}<br />
                        <label>หมู่ย่อย : </label> {{this.AJAX_DROPDOWN_SSO_EMPLOYEE_TSIC_4_TEXT}}<br />
                        <label>ประเภทอุตสาหกรรม : </label> {{this.AJAX_DROPDOWN_SSO_EMPLOYEE_TSIC_5_TEXT}}<br />
                        {{/xif}}
                    </p>
                </div>
            </div>

            {{/xif}}

            {{/xif}}
        </div>
        <div class="col-md-12">
            <a href="#ssoCollapse{{@@index}}" class="collapse-text" data-toggle="collapse" style="float:right;">ข้อมูลเพิ่มเติม</a>
        </div>
    </div>

    {{/each}}

    {{else}}

    <div class="row form-group pt10 pb10" style="margin: 0 0 5px 0 !important;">
        <div class="col-md-12 text-center">
            ไม่มีข้อมูลลูกจ้าง
        </div>
    </div>

    {{/xif}}
</script>