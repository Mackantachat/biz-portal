                        {{#xif " this.SSO_EMPLOYEE_FAMILY_STATUS_FALSE == 'SEPARATED' "}}
                        <label>สถานภาพครอบครัว : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYEE_FAMILY_STATUS_SEPARATED", "Apps_SingleForm", "SSO_EMPLOYEE_FAMILY_STATUS_SEPARATED")
                        {{/xif}}
                        {{/xif}}
                    </p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " typeof this.SSO_EMPLOYEE_HAS_KID !== 'undefined' " }}
                        {{#xif " this.SSO_EMPLOYEE_HAS_KID == 'false' "}}
                        <label>บุตร : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYEE_HAS_KID_FALSE", "Apps_SingleForm", "SSO_EMPLOYEE_HAS_KID_FALSE")
                        {{/xif}}
                        {{#xif " this.SSO_EMPLOYEE_HAS_KID == 'true' "}}
                        <label>บุตร : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYEE_HAS_KID_TRUE", "Apps_SingleForm", "SSO_EMPLOYEE_HAS_KID_TRUE")
                        {{/xif}}
                        {{/xif}}
                    </p>
                </div>
                {{#xif " this.SSO_EMPLOYEE_HAS_KID == 'true' "}}
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#each this.SSO_EMPLOYEE_HAS_KID_6Y_REPEATER}}
                        <label>บุตรคนที่ {{idxinc @@index}}</label> เกิดวันที่ {{this.SSO_EMPLOYEE_HAS_KID_6Y_REPEATER}}<br />
                        {{/each}}
                    </p>
                </div>
                {{/xif}}
            </div>
            <div class="row">
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " typeof this.SSO_EMPLOYEE_BODY_CONDITION_FALSE !== 'undefined' " }}
                        {{#xif " this.SSO_EMPLOYEE_BODY_CONDITION_FALSE == 'NORMAL' "}}
                        <label>สภาพร่างกาย : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYEE_BODY_CONDITION_NORMAL", "Apps_SingleForm", "SSO_EMPLOYEE_BODY_CONDITION_NORMAL")<br />
                        {{/xif}}
                        {{#xif " this.SSO_EMPLOYEE_BODY_CONDITION_FALSE == 'DISABILITY' "}}
                        <label>สภาพร่างกาย : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYEE_BODY_CONDITION_DISABILITY", "Apps_SingleForm", "SSO_EMPLOYEE_BODY_CONDITION_DISABILITY")<br />
                        {{/xif}}
                        {{/xif}}
                    </p>
                </div>
                {{#xif " this.SSO_EMPLOYEE_BODY_CONDITION_FALSE == 'DISABILITY' "}}
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#each this.SSO_EMPLOYEE_DISABILITY_CHECKBOXES}}
                        <label>{{idxinc @@index}}.</label>
                        {{#xif " this == 'VISUAL_DISABILITY' " }}
                        @ResourceHelper.GetResourceWordWithDefault("VISUAL_DISABILITY", "Apps_SingleForm", "VISUAL_DISABILITY")
                        {{/xif}}
                        {{#xif " this == 'HEARING_DISABILITY' " }}
                        @ResourceHelper.GetResourceWordWithDefault("HEARING_DISABILITY", "Apps_SingleForm", "HEARING_DISABILITY")
                        {{/xif}}
                        {{#xif " this == 'PHYSICAL_DISABILITY' " }}
                        @ResourceHelper.GetResourceWordWithDefault("PHYSICAL_DISABILITY", "Apps_SingleForm", "PHYSICAL_DISABILITY")
                        {{/xif}}
                        {{#xif " this == 'MENTALHEALTH_DISABILITY' " }}
                        @ResourceHelper.GetResourceWordWithDefault("MENTALHEALTH_DISABILITY", "Apps_SingleForm", "MENTALHEALTH_DISABILITY")
                        {{/xif}}
                        {{#xif " this == 'INTELLECTUAL_DISABILITY' " }}
                        @ResourceHelper.GetResourceWordWithDefault("INTELLECTUAL_DISABILITY", "Apps_SingleForm", "INTELLECTUAL_DISABILITY")
                        {{/xif}}
                        {{#xif " this == 'LEARNING_DISABILITY' " }}
                        @ResourceHelper.GetResourceWordWithDefault("LEARNING_DISABILITY", "Apps_SingleForm", "LEARNING_DISABILITY")
                        {{/xif}}
                        {{#xif " this == 'AUTISM_DISABILITY' " }}
                        @ResourceHelper.GetResourceWordWithDefault("AUTISM_DISABILITY", "Apps_SingleForm", "AUTISM_DISABILITY")
                        {{/xif}}
                        <br />
                        {{/each}}
                    </p>
                </div>
                {{/xif}}
            </div>
            <div class="row">
                <div class="col-md-12">
                    <p class="form-control-static">
                        {{#xif " typeof this.AJAX_DROPDOWN_SSO_EMPLOYEE_SELECT_HOSPITAL_1 !== 'undefined' && typeof this.AJAX_DROPDOWN_SSO_EMPLOYEE_SELECT_HOSPITAL_2 !== 'undefined' && typeof this.AJAX_DROPDOWN_SSO_EMPLOYEE_SELECT_HOSPITAL_3 !== 'undefined' " }}
