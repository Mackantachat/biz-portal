                        {{/xif}}
                        {{#xif " this.SSO_EMPLOYMENT_TYPE_FALSE == 'ETC' "}}
                        <label>ประเภทการจ้าง : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYMENT_TYPE_ETC", "Apps_SingleForm", "SSO_EMPLOYMENT_TYPE_ETC") ({{this.SSO_EMPLOYMENT_TYPE_ETC_FALSE_TEXT}})
                        {{/xif}}
                        {{/xif}}
                    </p>
                </div>
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " typeof this.SSO_EMPLOYEE_WAGE_FALSE !== 'undefined' " }}
                        <label>จำนวนเงินค่าจ้าง (บาท) : </label> {{this.SSO_EMPLOYEE_WAGE_FALSE}}
                        {{/xif}}
                    </p>
                </div>
            </div>

            {{#xif " typeof this.SSO_EMPLOYEE_IS_MULTIPLE_WORKPLACE_FALSE !== 'undefined' && this.SSO_EMPLOYEE_IS_MULTIPLE_WORKPLACE_FALSE == 'true' " }}
            <div class="row">
                <div class="col-md-6">
                    <p class="form-control-static">
                        <label>ทำงานกับนายจ้างหลายแห่ง : </label> {{this.SSO_EMPLOYEE_IS_MULTIPLE_WORKPLACE_FALSE_TEXT}}
                    </p>
                </div>
            </div>
            {{/xif}}
            <div class="row">
                <div class="col-md-12">
                    <p class="form-control-static">
                        {{#xif " typeof this.SSO_EMPLOYEE_FAMILY_STATUS_FALSE !== 'undefined' " }}
                        {{#xif " this.SSO_EMPLOYEE_FAMILY_STATUS_FALSE == 'SINGLE' "}}
                        <label>สถานภาพครอบครัว : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYEE_FAMILY_STATUS_SINGLE", "Apps_SingleForm", "SSO_EMPLOYEE_FAMILY_STATUS_SINGLE")
                        {{/xif}}
                        {{#xif " this.SSO_EMPLOYEE_FAMILY_STATUS_FALSE == 'MARRIED' "}}
                        <label>สถานภาพครอบครัว : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYEE_FAMILY_STATUS_MARRIED", "Apps_SingleForm", "SSO_EMPLOYEE_FAMILY_STATUS_MARRIED")
                        {{/xif}}
                        {{#xif " this.SSO_EMPLOYEE_FAMILY_STATUS_FALSE == 'WIDOW' "}}
                        <label>สถานภาพครอบครัว : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYEE_FAMILY_STATUS_WIDOW", "Apps_SingleForm", "SSO_EMPLOYEE_FAMILY_STATUS_WIDOW")
