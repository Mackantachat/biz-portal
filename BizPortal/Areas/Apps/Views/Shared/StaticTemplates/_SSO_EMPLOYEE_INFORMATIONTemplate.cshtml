            <div class="row">
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " typeof this.AJAX_DROPDOWN_SSO_EMPLOYEE_BRANCH_TRUE_TEXT !== 'undefined' " }}
                        <label>สาขาหรือสถานที่ทำงานของลูกจ้าง : </label> {{this.AJAX_DROPDOWN_SSO_EMPLOYEE_BRANCH_TRUE_TEXT}}
                        {{/xif}}
                    </p>
                </div>
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " typeof this.SSO_EMPLOYEE_FIRSTDAYOFWORK_TRUE !== 'undefined' " }}
                        <label>วันที่ผู้ประกันตนเข้าทำงาน : </label> {{this.SSO_EMPLOYEE_FIRSTDAYOFWORK_TRUE}}
                        {{/xif}}
                    </p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " typeof this.SSO_EMPLOYMENT_TYPE_TRUE !== 'undefined' " }}
                        {{#xif " this.SSO_EMPLOYMENT_TYPE_TRUE == 'DAILY' "}}
                        <label>ประเภทการจ้าง : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYMENT_TYPE_DAILY", "Apps_SingleForm", "SSO_EMPLOYMENT_TYPE_DAILY")
                        {{/xif}}
                        {{#xif " this.SSO_EMPLOYMENT_TYPE_TRUE == 'MONTHLY' "}}
                        <label>ประเภทการจ้าง : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYMENT_TYPE_MONTHLY", "Apps_SingleForm", "SSO_EMPLOYMENT_TYPE_MONTHLY")
                        {{/xif}}
                        {{#xif " this.SSO_EMPLOYMENT_TYPE_TRUE == 'HOURLY' "}}
                        <label>ประเภทการจ้าง : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYMENT_TYPE_HOURLY", "Apps_SingleForm", "SSO_EMPLOYMENT_TYPE_HOURLY")
                        {{/xif}}
                        {{#xif " this.SSO_EMPLOYMENT_TYPE_TRUE == 'ETC' "}}
