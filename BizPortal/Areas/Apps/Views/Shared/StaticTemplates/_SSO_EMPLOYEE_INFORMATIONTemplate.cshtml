                        {{/xif}}
                    </p>
                </div>
            </div>
            {{else}}
            <div class="row">
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " typeof this.AJAX_DROPDOWN_SSO_EMPLOYEE_BRANCH_FALSE_TEXT !== 'undefined' " }}
                        <label>สาขาหรือสถานที่ทำงานของลูกจ้าง : </label> {{this.AJAX_DROPDOWN_SSO_EMPLOYEE_BRANCH_FALSE_TEXT}}
                        {{/xif}}
                    </p>
                </div>
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " typeof this.SSO_EMPLOYEE_FIRSTDAYOFWORK_FALSE !== 'undefined' " }}
                        <label>วันที่ผู้ประกันตนเข้าทำงาน : </label> {{this.SSO_EMPLOYEE_FIRSTDAYOFWORK_FALSE}}
                        {{/xif}}
                    </p>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <p class="form-control-static">
                        {{#xif " typeof this.SSO_EMPLOYMENT_TYPE_FALSE !== 'undefined' " }}
                        {{#xif " this.SSO_EMPLOYMENT_TYPE_FALSE == 'DAILY' "}}
                        <label>ประเภทการจ้าง : </label> @ResourceHelper.GetResourceWordWithDefault("SSO_EMPLOYMENT_TYPE_DAILY", "Apps_SingleForm", "SSO_EMPLOYMENT_TYPE_DAILY")
