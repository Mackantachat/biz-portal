@using BizPortal.Utils.Helpers

<script id="APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTIONTemplate" type="text/x-handlebars-template">
    {{#xif " data.length > 0 " }}
    {{#each data}}

    <div class="row form-group pt10 pb10" style="margin: 0 0 5px 0 !important; border:solid 1px #000000; border-radius: 4px;">
        <div class="col-md-12">
            {{#xif " fieldExist(this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_LAND_ID) " }}
            <label>เลขที่โฉนด / สปก.: </label> {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_LAND_ID}}
            {{/xif}}
            {{#xif " fieldExist(this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_LAND_NO) " }}
            <label>เลขที่ดิน: </label> {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_LAND_NO}}
            {{/xif}}
            {{#xif " fieldExist(this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_LAND_CODE) " }}
            <label>ระวาง: </label> {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_LAND_CODE}}
            {{/xif}}
            {{#xif " fieldExist(this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS_OWNER) " }}
            <label>ชื่อผู้มีกรรมสิทธิ์: </label> {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS_OWNER}}
            {{/xif}}
            {{#xif " fieldExist(this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_AREA) " }}
            <label>พื้นที่เก็บรถ (ตารางเมตร): </label> {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_AREA}}
            {{/xif}}
            {{#xif " fieldExist(this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_CNT_TRUCK_STORE) " }}
            <label>เก็บรถได้ (คัน): </label> {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_CNT_TRUCK_STORE}}
            {{/xif}}
            {{#xif " fieldExist(this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_AREA_RAI) " }}
            <label>พื้นที่ตามเอกสาร ไร่: </label> {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_AREA_RAI}}
            {{/xif}}
            {{#xif " fieldExist(this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_AREA_NKAN) " }}
            <label>พื้นที่ตามเอกสาร งาน: </label> {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_AREA_NKAN}}
            {{/xif}}
            {{#xif " fieldExist(this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_AREA_TARANKWA) " }}
            <label>พื้นที่ตามเอกสาร ตารางวา: </label> {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_AREA_TARANKWA}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>เลขที่: </label> {{this.ADDRESS_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_MOO_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>หมู่ที่: </label> {{this.ADDRESS_MOO_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_VILLAGE_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>หมู่บ้าน: </label> {{this.ADDRESS_VILLAGE_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_SOI_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>ซอย: </label> {{this.ADDRESS_SOI_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_BUILDING_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>อาคาร: </label> {{this.ADDRESS_BUILDING_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_ROOMNO_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>ห้องเลขที่: </label> {{this.ADDRESS_ROOMNO_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_FLOOR_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>ชั้น: </label> {{this.ADDRESS_FLOOR_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_ROAD_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>ถนน: </label> {{this.ADDRESS_ROAD_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}</>
            {{/xif}}
            <label>ตำบล/แขวง: </label> {{this.ADDRESS_TUMBOL_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS_TEXT}}
            <label>อำเภอ/เขต: </label> {{this.ADDRESS_AMPHUR_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS_TEXT}}
            <label>จังหวัด: </label> {{this.ADDRESS_PROVINCE_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS_TEXT}}
            {{#xif " fieldExist(this.ADDRESS_POSTCODE_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>รหัสไปรษณีย์: </label> {{this.ADDRESS_POSTCODE_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_TELEPHONE_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>โทรศัพท์: </label> {{this.ADDRESS_TELEPHONE_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_TELEPHONE_EXT_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <label>ต่อ:</label> {{this.ADDRESS_TELEPHONE_EXT_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_FAX_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <nobr><label>โทรสาร:</label> {{this.ADDRESS_FAX_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}</nobr>
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_LAT_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <nobr><label>ละติจูด:</label> {{this.ADDRESS_LAT_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}</nobr>
            {{/xif}}
            {{#xif " fieldExist(this.ADDRESS_LNG_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS) " }}
            <nobr><label>ลองจิจูด:</label> {{this.ADDRESS_LNG_APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_ADDRESS}}</nobr>
            {{/xif}}
        </div>
        @{
            if (ViewBag.RequestIdentityType == UserTypeEnum.Citizen)
            {
                <Text>
                <div class="col-md-12">
                    <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_TRANSPORT_NON_REGULAR_TRUCK.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_TYPE_CITIZEN  :</label>
                    {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_TYPE_CITIZEN_OPTION__RADIO_TEXT}}
                </div>
                {{#xif " this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_TYPE_CITIZEN_OPTION != 'INFORMATION_STORE_BUILDING_TYPE_OPTION__OWNED' " }}
                <div class="col-md-12">
                    <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_TRANSPORT_NON_REGULAR_TRUCK.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_RENTING_OWNER_TYPE  :</label>
                    {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_RENTING_OWNER_TYPE_OPTION__RADIO_TEXT}}
                </div>
                {{/xif}}
                </Text>
            }
            else if (ViewBag.RequestIdentityType == UserTypeEnum.Juristic)
            {
                <Text>
                <div class="col-md-12">
                    <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_TRANSPORT_NON_REGULAR_TRUCK.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_TYPE_JURISTIC  :</label>
                    {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_TYPE_JURISTIC_OPTION__RADIO_TEXT}}
                </div>
                {{#xif " this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_TYPE_JURISTIC_OPTION != 'INFORMATION_STORE_BUILDING_TYPE_OPTION__OWNED' " }}
                <div class="col-md-12">
                    <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_TRANSPORT_NON_REGULAR_TRUCK.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_RENTING_OWNER_TYPE  :</label>
                    {{this.APP_TRANSPORT_NON_REGULAR_TRUCK_MAINTENANCE_SECTION_BUILDING_RENTING_OWNER_TYPE_OPTION__RADIO_TEXT}}
                </div>
                {{/xif}}
                </Text>
            }
        }
    </div>

    {{/each}}
    {{else}}
    <div class="row form-group pt10 pb10" style="margin: 0 0 5px 0 !important; border:solid 1px #000000; border-radius: 4px;">
        <div class="col-md-12 text-center">
            @Html.Raw(ResourceHelper.GetResourceWordWithDefault(Model.EmptyDataMessage, StringHelper.DefaultIfEmpty(Model.ResourceName, "Apps_SingleForm_Template"), Model.EmptyDataMessage))
        </div>
    </div>
    {{/xif}}
</script>