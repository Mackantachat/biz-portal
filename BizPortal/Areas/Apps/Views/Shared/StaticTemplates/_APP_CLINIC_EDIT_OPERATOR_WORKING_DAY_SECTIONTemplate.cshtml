@using BizPortal.Utils.Helpers

<script id="APP_CLINIC_EDIT_OPERATOR_WORKING_DAY_SECTIONTemplate" type="text/x-handlebars-template">
    {{#xif " data.length > 0 " }}
    {{#each data}}

    <div class="row form-group pt10 pb10" style="margin: 0 0 5px 0 !important; border:solid 1px #000000; border-radius: 4px;">
        <div class="col-md-8">
            <div class="col-md-4">
                <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_CLINIC_EDIT.APP_CLINIC_EDIT_OPERATOR_WORKING_DAY_SECTION_DAY :</label>
                {{this.DROPDOWN_APP_CLINIC_EDIT_OPERATOR_WORKING_DAY_SECTION_DAY_TEXT}}
            </div>
            <div class="col-md-4">
                <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_CLINIC_EDIT.APP_CLINIC_EDIT_OPERATOR_WORKING_DAY_SECTION_START_TIME :</label>
                {{this.DROPDOWN_APP_CLINIC_EDIT_OPERATOR_WORKING_DAY_SECTION_START_TIME_TEXT}}
            </div>
            <div class="col-md-4">
                <label>@BizPortal.Resources.PermitResource.RESOURCE_APP_CLINIC_EDIT.APP_CLINIC_EDIT_OPERATOR_WORKING_DAY_SECTION_END_TIME :</label>
                {{this.DROPDOWN_APP_CLINIC_EDIT_OPERATOR_WORKING_DAY_SECTION_END_TIME_TEXT}}
            </div>
        </div>
    </div>

    {{/each}}
    {{else}}
    <div class="row form-group pt10 pb10" style="margin: 0 0 5px 0 !important; border:solid 1px #000000; border-radius: 4px;">
        <div class="col-md-12 text-center">
            @Html.Raw(ResourceHelper.GetResourceWordWithDefault(Model.EmptyDataMessage, StringHelper.DefaultIfEmpty(Model.ResourceName, "Apps_SingleForm_Template"), Model.EmptyDataMessage))
        </div>
    </div>
    {{/xif}}
</script>