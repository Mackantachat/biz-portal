            $("#ajax_dropdown_SSO_EMPLOYEE_TSIC_5").val("").trigger('change');
            $("#ajax_dropdown_SSO_EMPLOYEE_TSIC_5 option[value]").remove();
            $("#SSO_TSIC_MULTIPLIER").val("");

            if (x <= 2) {
                $("#ajax_dropdown_SSO_EMPLOYEE_TSIC_4").val("").trigger('change');
                $("#ajax_dropdown_SSO_EMPLOYEE_TSIC_4 option[value]").remove();
            }
            if (x <= 1) {
                $("#ajax_dropdown_SSO_EMPLOYEE_TSIC_3").val("").trigger('change');
                $("#ajax_dropdown_SSO_EMPLOYEE_TSIC_3 option[value]").remove();
            }
