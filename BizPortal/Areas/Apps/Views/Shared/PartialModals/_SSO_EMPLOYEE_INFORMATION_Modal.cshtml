<script type="text/javascript">
    $(function () {
        $("#SSO_EMPLOYEE_TITLE_TRUE, #SSO_EMPLOYEE_TITLE_FALSE").select2({
            data: [{ "id": "01", "text": "นาย" }, { "id": "02", "text": "นาง" }, { "id": "03", "text": "นางสาว" }, { "id": "11", "text": "Mr." }, { "id": "12", "text": "Mrs." }, { "id": "13", "text": "Miss" }],
            allowClear: true,
            placeholder: '',
            width: '100%'
        });

        $("#frmSSO_EMPLOYEE_INFORMATION").submit(function (e) {
            var errors = 0;
            if ($("#SSO_EMPLOYEE_REG_STATUS").val() != "true" && $("#SSO_EMPLOYEE_REG_STATUS").val() != "false") {
                $("#SSO_EMPLOYEE_CITIZEN_ID-error").text("กรุณาตรวจสอบเลขบัตรประจำตัวประชาชน 13 หลัก");
                errors++;
            }
            if (!$(this).valid()) {
                errors++;
            }
            if (errors == 0) {
                return true;
            }
            e.preventDefault();
        });

        function formDataClear() {
            $form = $("#frmSSO_EMPLOYEE_INFORMATION");
            $(":input", $form).each(function () {
                var $this = $(this);
                if ($this[0].name != "SSO_EMPLOYEE_CITIZEN_ID") {
                    if (!$this.is(':radio') && !$this.is(':checkbox')) {
                        $this.val('');
                    } else {
                        $this.prop('checked', false);
                    }
                    if ($this[0].tagName === 'SELECT') {
                        $this.trigger("change");
                    }
                }
            });
        }

        function toggleDiv(x, y) {
            $("#ssotrue").prop("hidden", x);
            $("#ssofalse").prop("hidden", y);
        }

        function toggleBtn(x) {
            $("#SSO_EMPLOYEE_CITIZEN_ID").prop("readonly", x);
            $("#btnStatusCheck").prop("hidden", x);
            $("#btnStatusRemove").prop("hidden", !x);
            $("#ssoTSIC").prop("hidden", !x);
        }

        $("#SSO_EMPLOYEE_CITIZEN_ID").mask("0-0000-00000-00-0", {
            clearIfNotMatch: true
        });

        $("#btnStatusCheck").click(function () {
            formDataClear();
            $("#SSO_EMPLOYEE_CITIZEN_ID-error").empty();
            $("#SSO_EMPLOYEE_REG_STATUS-error").empty();
            var $field = $("#SSO_EMPLOYEE_CITIZEN_ID");
            var citid = $field.val().replace(/-/g, "");
            if (citid.length == 13) {
                showLoader(true);
                $.ajax({
                    url: '@Url.Content("~/Api/v2/SSO/EmployeeRegStatusCheck")',
                    data: {
                        IdentityID: citid
                    },
                    success: function (data) {
                        if (data) {
                            //console.log(data);
                            if (data.result) {
                                toggleDiv(false, true);
                                $("#SSO_EMPLOYEE_REG_STATUS").val("true");
                            } else {
                                toggleDiv(true, false);
                                $("#SSO_EMPLOYEE_REG_STATUS").val("false");
                            }
                            $field.prop("readonly", true);
                            toggleBtn(true);
                        } else {
                            toggleDiv(true, true);
                            $("#SSO_EMPLOYEE_REG_STATUS").val("");
                            $("#SSO_EMPLOYEE_CITIZEN_ID-error").text("ไม่พบเลขบัตรประจำตัวประชาชนนี้ กรุณาตรวจสอบใหม่อีกครั้ง").css('display', 'block');
                        }
                        showLoader(false);
                    },
                    error: function () {
                        //console.log("error");
                        $("#SSO_EMPLOYEE_REG_STATUS").val("");
                        $("#SSO_EMPLOYEE_CITIZEN_ID-error").text("ไม่สามารถตรวจสอบสถานะการจะทะเบียนของลูกจ้างได้ กรุณาลองใหม่อีกครั้ง").css('display', 'block');
                        showLoader(false);
                    }
                });
            } else {
                $("#SSO_EMPLOYEE_CITIZEN_ID-error").text("กรุณากรอกเลขบัตรประจำตัวประชาชน 13 หลัก").css('display', 'block');
            }
        });

        $("#btnStatusRemove").click(function () {
            formDataClear();
            toggleDiv(true, true);
            toggleBtn(false);
        });

