<script id="appRangeItemTemplate" type="text/x-custom-template">
    <div class="blog-item">
        <div class="head">
            <p class="head" style="width:100%;">{0}</p>
            @*<p>{1}</p>*@
        </div>
        <div class="price">
            <p class="num">{3} - {4} @Resources.PermitSummary.TEXT_BAHT</p>
            <p class="day">{2} @Resources.PermitSummary.TEXT_DAYS</p>
